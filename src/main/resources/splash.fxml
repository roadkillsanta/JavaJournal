<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="600.0" prefWidth="1200.0">
      <children>
         <ImageView fitHeight="625.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@splash.png" />
            </image>
            <viewport>
               <Rectangle2D />
            </viewport>
         </ImageView>
         <Button fx:id="open" layoutX="555.0" layoutY="462.0" mnemonicParsing="false" text="Open Existing" />
         <Button fx:id="create" layoutX="561.0" layoutY="412.0" mnemonicParsing="false" text="Create New" />
      </children></AnchorPane>
</Scene>
